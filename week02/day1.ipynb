    "#### Vowels counter"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "54d7fcd8",
   "metadata": {},
   "outputs": [],
   "source": [
    "def vowelCounter(str):\n",
    "    vowels = \"aeiou\"\n",
    "    count = 0\n",
    "    for char in str.lower():\n",
    "        if char in vowels:\n",
    "            count += 1\n",
    "    return count\n",
    "\n",
    "word = str(input(\"Enter a word: \"))\n",
    "print(vowelCounter(word))"
   ]
  }
